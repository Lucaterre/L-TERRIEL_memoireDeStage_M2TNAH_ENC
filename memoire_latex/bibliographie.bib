	keywords = {Projets patrimoniaux numériques (dont Lectaurep)},
	file = {Full Text PDF:/Users/lucasterriel/Zotero/storage/S9S9WNAQ/Rochebouet - 2017 - Le texte médiéval à l’épreuve du numérique.pdf:application/pdf;Snapshot:/Users/lucasterriel/Zotero/storage/JZMAZFR7/Rochebouet - 2017 - Le texte médiéval à l’épreuve du numérique.html:text/html}
